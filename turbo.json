{
   "$schema": "https://turbo.build/schema.json",
   "pipeline": {
      "build": {
         "outputs": ["dist/**"],
         "dependsOn": ["check:eslint", "check:typescript", "^build"]
      },
      "check:eslint": {},
      "check:typescript": {},
      "clean": {
         "cache": false
      },
      "dev": {
         "cache": false
      },
      "start": {
         "outputs": [],
         "dependsOn": ["^build", "^test"]
      },
      "test": {
         "outputs": [],
         "dependsOn": ["^build"]
      }
   }
}
